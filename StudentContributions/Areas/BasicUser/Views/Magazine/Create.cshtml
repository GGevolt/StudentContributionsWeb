@model StudentContributions.Models.Models.Magazine

@{
    ViewData["Title"] = "Create Magazine";
}

<h1>Create Magazine</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="MagazineName" class="control-label"></label>
        <input asp-for="MagazineName" class="form-control" />
        <span asp-validation-for="MagazineName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ClosureDate" class="control-label"></label>
        <input asp-for="ClosureDate" class="form-control" />
        <span asp-validation-for="ClosureDate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="FacultyID" class="control-label"></label>
        <input asp-for="FacultyID" class="form-control" />
        <span asp-validation-for="FacultyID" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="SemesterID" class="control-label"></label>
        <input asp-for="SemesterID" class="form-control" />
        <span asp-validation-for="SemesterID" class="text-danger"></span>
    </div>
    <input type="submit" value="Create" class="btn btn-primary" />
</form>

<div>
    <a href="@Url.Action("Index")">Back to List</a>
</div>
