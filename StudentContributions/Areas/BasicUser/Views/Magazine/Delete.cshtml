@model StudentContributions.Models.Models.Magazine

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Magazine</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>ID</dt>
        <dd>@Html.DisplayFor(model => model.ID)</dd>
        <dt>Magazine Name</dt>
        <dd>@Html.DisplayFor(model => model.MagazineName)</dd>
        <dt>Closure Date</dt>
        <dd>@Html.DisplayFor(model => model.ClosureDate)</dd>
        <dt>Faculty ID</dt>
        <dd>@Html.DisplayFor(model => model.FacultyID)</dd>
        <dt>Semester ID</dt>
        <dd>@Html.DisplayFor(model => model.SemesterID)</dd>
    </dl>
    <form asp-action="DeleteConfirmed" method="post">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
